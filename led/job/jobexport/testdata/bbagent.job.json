    "env_vars": [
      {
        "key": "SWARMING_TASK_TEMPLATE_NAME",
        "value": "test_canary"
      }
    ],
    "env_prefixes": [
      {
        "key": "PATH",
        "values": [
          "cipd_bin_packages",
          "cipd_bin_packages/bin",
          "cipd_bin_packages/cpython",
          "cipd_bin_packages/cpython/bin",
          "cipd_bin_packages/cpython3",
